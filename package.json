{
  "name": "scheduling-app",
  "private": true,
  "version": "0.0.1",
  "type": "commonjs",
  "scripts": {
    "dev:client": "vite --port 5173",
    "dev:server": "ts-node-dev --respawn --transpile-only src/server/index.ts",
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "deploy:staging": "docker-compose -f docker-compose.staging.yml up -d",
    "deploy:production": "docker-compose -f docker-compose.production.yml up -d",
    "dev:docker": "docker-compose -f docker-compose.local.yml up",
    "dev:docker:build": "docker-compose -f docker-compose.local.yml up --build",
    "dev:docker:down": "docker-compose -f docker-compose.local.yml down"
  },
  "dependencies": {
    "axios": "^1.6.7",
    "cors": "^2.8.5",
    "date-fns": "^2.30.0",
    "dotenv": "^16.4.5",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.22.0",
    "socket.io": "^4.7.4",
    "socket.io-client": "^4.7.4"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.5.9",
    "autoprefixer": "^10.4.17",
    "postcss": "^8.4.33",
    "tailwindcss": "^3.4.1",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.11.16",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react": "^4.2.1",
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.3",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.2.2",
    "vite": "^5.0.8"
  }
}
